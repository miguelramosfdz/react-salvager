{
  "name": "react-salvager",
  "version": "1.0.0",
  "description": "A React component that recycles row elements.",
  "main": "lib/react-salvager.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "lint": "eslint src test",
    "build:umd": "webpack src/Salvager.js dist/Salvager.js --config webpack.config.dev.js",
    "test:spec": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register",
    "test:watch": "npm run test:spec -- --watch",
    "start:server": "node server.js",
    "start": "parallelshell 'npm run start:server' 'npm run test:watch'"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4",
    "parallelshell": "^2.0.0",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0"
  },
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "classnames": "^2.2.1",
    "lodash.isfunction": "^3.0.6"
  }
}
